{
  "name": "vitfl-portfolio",
  "version": "1.0.0",
  "description": "VitFL front-end developer portfolio",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node server/index.js",
    "client": "npm start --prefix react-ui",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client \"",
    "install-deps": "npm install && npm install --prefix react-ui",
    "heroku-postbuild": "npm install --prefix react-ui && npm run build --prefix react-ui"
  },
  "dependencies": {
    "express": "^4.17.0",
    "mongoose": "^5.5.11",
    "node-sass": "^4.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VitFL/vitfl-portfolio.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0"
  }
}
