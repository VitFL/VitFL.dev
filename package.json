{
  "name": "vitfl-portfolio",
  "version": "1.0.0",
  "description": "VitFL front-end developer portfolio",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "client": "cd react-ui && npm start",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client \"",
    "build": "npm install && cd react-ui && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "express": "^4.17.0",
    "mongoose": "^5.5.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VitFL/vitfl-portfolio.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0"
  }
}
