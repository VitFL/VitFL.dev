{
  "name": "vitfl-portfolio",
  "version": "1.0.0",
  "description": "VitFL front-end developer portfolio",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "nodemon --exec babel-node server/index.js",
    "client": "npm start --prefix react-ui",
    "server": "nodemon --exec babel-node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client \"",
    "install-deps": "npm install && npm install --prefix react-ui",
    "heroku-postbuild": "npm install --prefix react-ui && npm run build --prefix react-ui"
  },
  "dependencies": {
    "gravatar": "^1.8.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "graphql": "^14.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VitFL/vitfl-portfolio.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  }
}
