{
  "name": "vitfl.dev",
  "version": "0.1.0",
  "description": "vitfl.dev portfolio website",
  "main": "server.js",
  "scripts": {
    "build-ts": "tsc",
    "start": "npm run build-ts && node dist/server.js",
    "client": "npm start --prefix client",
    "server": "npm run build-ts && nodemon dist/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Vitaliy Tkachuk (VitFL)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.7"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/node": "^13.1.6",
    "typescript": "^3.2.2"
  }
}
